<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyb.springcloud.mapper.ForexExposureMapper">
    <resultMap id="BaseResultMap" type="com.dyb.springcloud.pojo.ForexExposure">
        <result column="id" property="id"/>
        <result column="header_id" property="headerId"/>
        <result column="row_index" property="rowIndex"/>
        <result column="business_type" property="businessType"/>
        <result column="account_subject" property="accountSubject"/>
        <result column="usd" property="usd"/>
        <result column="gbp" property="gbp"/>
        <result column="hkd" property="hkd"/>
        <result column="cny" property="cny"/>
        <result column="total" property="total"/>
        <result column="year_balance" property="yearBalance"/>
        <result column="comment" property="comment"/>
        <result column="operation" property="operation"/>
        <result column="update_by" property="updateBy"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_by_name" property="updateByName"/>
        <result column="create_by" property="createBy"/>
        <result column="create_by_name" property="createByName"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <insert id="insert">
            insert into
            fund_report_forex_exposure_row(header_id, row_index, business_type, account_subject,usd,gbp,hkd,cny,total,year_balance,comment,operation)
            VALUES
            (#{model.headerId},
            #{model.rowIndex},
            #{model.businessType},
            #{model.accountSubject},
            #{model.usd},
            #{model.gbp},
            #{model.hkd},
            #{model.cny},
            #{model.total},
            #{model.yearBalance},
            #{model.comment},
            #{model.operation})
    </insert>
</mapper>